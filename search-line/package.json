{
  "name": "search-line",
  "displayName": "search-line",
  "description": "Multi-cursor character finder and selector for fast navigation.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.99.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GustavoSept/vscode_plugins.git"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.findChar",
        "title": "Find Character"
      },
      {
        "command": "extension.findCharAndSelect",
        "title": "Find Character and Select"
      },
      {
        "command": "extension.findCharBackward",
        "title": "Find Character"
      },
      {
        "command": "extension.findCharAndSelectBackward",
        "title": "Find Character and Select"
      }
    ],
    "keybindings": [
      {
        "command": "extension.findChar",
        "key": "ctrl+s f",
        "when": "editorTextFocus"
      },
      {
        "command": "extension.findCharAndSelect",
        "key": "ctrl+s shift+f",
        "when": "editorTextFocus"
      },
      {
        "command": "extension.findCharBackward",
        "key": "ctrl+s alt+f",
        "when": "editorTextFocus"
      },
      {
        "command": "extension.findCharAndSelectBackward",
        "key": "ctrl+s alt+shift+f",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "package": "vsce package",
    "vsix": "npm run compile && vsce package"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.99.0",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.23.0",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "@vscode/vsce": "^3.3.2"
  }
}